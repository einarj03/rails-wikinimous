<%= render 'navbar' %>
<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <% @articles.each do |article| %>
        <h2 class="title"><%= link_to article.title, article_path(article) %></h2>
        <p class="content"><%= article.content %></p>
      <% end %>
    </div>
  </div>
</div>
